#include <iostream>
#include <stack>
using namespace std;
void rev(string arr){
    stack <char> s;
    
    for(int i = 0 ; i < arr.length() ; i++){
        char c = arr[i];
        s.push(c);
    }
    
    while(!s.empty()){
        cout<<s.top();
        s.pop();
    }
    
        cout<<endl;
    
}
int main()
{
  string word = "hello";
   cout << "Original: " << word << endl;
    rev(word);
  
  

    return 0;
}
